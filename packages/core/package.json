{
	"name": "@watthem/fieldtest",
	"version": "1.0.0",
	"description": "Open source validation toolkit for data pipelines, APIs, and integrations. Built with TypeScript for modern development workflows.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"sideEffects": false,
	"files": ["dist/**", "README.md", "CHANGELOG.md"],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf dist",
		"lint": "eslint src --ext .ts",
		"test": "vitest run"
	},
	"keywords": [
		"validation",
		"typescript",
		"schema",
		"data-pipeline",
		"api-testing", 
		"integration-testing",
		"standard-schema",
		"quality-assurance",
		"ci-cd"
	],
	"homepage": "https://fieldtest.watthem.blog",
	"repository": {
		"type": "git",
		"url": "https://github.com/watthem/fieldtest.git"
	},
	"bugs": {
		"url": "https://github.com/watthem/fieldtest/issues"
	},
	"author": "Matthew Hendricks <hello@matthewhendricks.net>",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/node": "^18.17.15",
		"eslint": "^8.49.0",
		"rimraf": "^5.0.0",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.4"
	},
	"dependencies": {
		"gray-matter": "^4.0.3"
	}
}
